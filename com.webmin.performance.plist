<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.webmin.performance</string>
    <key>ProgramArguments</key>
    <array>
        <string>/opt/webmin-performance/webmin-performance-optimizer.sh</string>
        <string>monitor</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StartInterval</key>
    <integer>300</integer>
    <key>StandardOutPath</key>
    <string>/var/log/webmin-performance.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/webmin-performance-error.log</string>
    <key>WorkingDirectory</key>
    <string>/opt/webmin-performance</string>
</dict>
</plist>
