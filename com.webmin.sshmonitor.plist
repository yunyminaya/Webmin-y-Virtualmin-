<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.webmin.sshmonitor</string>
    <key>ProgramArguments</key>
    <array>
        <string>/opt/webmin-self-healing/ssh-monitor.sh</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/var/log/webmin-ssh-monitor.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/webmin-ssh-monitor-error.log</string>
</dict>
</plist>
